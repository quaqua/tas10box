$('#tas10-main-container').tas10Container( 'append', {
  title: '<%= @query_script.name %>',
  id: '_<%= @query_script.id %>', 
  content: '<%= raw escape_javascript(render("documents/find")) %>'
  }, function( container ){
    $(container).find('.tas10-list').tas10List({ url: '<%= find_documents_path %><%= raw @query_script.query %>' });
  }
);