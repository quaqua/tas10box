$('#tas10-main-container').tas10Container( 'append', {
  replace: true,
  title: '<%= @doc.name %>', 
  id: '<%= @doc.id %>', 
  content: '<%= escape_javascript render('labels/show') %>'
  }, function( container ){
    $(container).find('.tas10-list').tas10List({ url: '<%= children_for_document_path(@doc) %>' });
  }
);